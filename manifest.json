{
  "manifest_version": 3,
  "name": "Tab Timer Tracker",
  "version": "1.0",
  "description": "Tracks how much time you spend on each tab.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Tab Timer",
    "default_popup": "popup.html"
  }
}